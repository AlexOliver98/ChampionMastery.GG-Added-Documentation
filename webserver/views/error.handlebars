	{{/content}}
	{{#content "title"}}ChampionMastery.GG - {{T.[Error]}}{{/content}}
	{{#content "body"}}
		<div id="container">
			<!-- TODO localize 404 error and default message -->
			<h1>{{T.[Error]}}</h1>
